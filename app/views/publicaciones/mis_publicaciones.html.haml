#region-publicaciones.container

  .region
    .region-title
      %h1.page-title= page_title!('Mis Publicaciones')

    .region-inner
      - if current_usuario
        .admin-control
          = link_to 'Crear Publicacion', new_publicacion_path, class: 'btn btn-primary'

  .region
    .region-inner
      #publicaciones
        = render @publicaciones
